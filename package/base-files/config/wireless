
config wifi-device 'radio0'
	option type 'mac80211'
	option channel '11'
	option hwmode '11ng'
	option path 'bcma0:0'
	option htmode 'HT20'
	list ht_capab 'GF'
	list ht_capab 'SHORT-GI-20'
	list ht_capab 'SHORT-GI-40'
	option txpower '19'
	option country '00'

config wifi-iface
	option device 'radio0'
	option network 'lan'
	option mode 'ap'
	option ssid 'OpenWrt'
	option encryption 'none'

